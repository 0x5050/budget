{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~*": ["./*"],
      "~/*": ["./*"],
      "@*": ["./*"],
      "@/*": ["./*"],
      "#*": ["./components/*"],
      "#/*": ["./components/*"],
      "##*": ["./pages/*"],
      "##/*": ["./pages/*"]
    }
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
